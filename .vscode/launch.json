  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "stock",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\main.js",
      "args":["--module=deal"]
      // "program": "${workspaceFolder}\\test.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ts-stock",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register"
      ],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}\\src\\main.ts",
      "args":["--module=deal"]
      // "program": "${workspaceFolder}\\test.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test111",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\app\\analyze\\klines\\Uline.js",
      // "program": "${workspaceFolder}\\test.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "serve",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\api\\main.js",
      "args":["--port=9527"]
      // "program": "${workspaceFolder}\\test.js"
    }
  ]
}